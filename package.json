{
  "name": "gatsby-test",
  "description": "Gatsby testing",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>, and Eric Hodges",
  "dependencies": {
    "gatsby": "^1.9.247",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-react-helmet": "^2.0.10",    
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "watch:sass": "node-sass src/layouts/sass/main.scss src/layouts/css/style.css -w",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "start": "npm-run-all --parallel develop watch:sass",
    "format": "prettier --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.12.0"
  }
}
